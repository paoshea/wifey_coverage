{
  "extends": ["next/core-web-vitals"],
  "plugins": ["eslint-comments"],
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "eslint-comments/no-unused-disable": "error"
  }
}
